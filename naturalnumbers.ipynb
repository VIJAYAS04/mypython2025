      "source": [
        "\n",
        "def sum_of_squares(N):\n",
        "    total = 0\n",
        "    for i in range(1, N + 1):\n",
        "        total += i ** 2\n",
        "    return total\n",
        "\n",
        "\n",
        "N = int(input(\"Enter a number N: \"))\n",
        "\n",
        "\n",
        "result = sum_of_squares(N)\n",
        "print(f\"The sum of squares of the first {N} natural numbers is {result}.\")\n",
        "\n"
      ],
