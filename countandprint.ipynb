      "source": [
        "\n",
        "def count_digits(number):\n",
        "\n",
        "    return len(str(abs(number)))\n",
        "\n",
        "\n",
        "num = int(input(\"Enter an integer: \"))\n",
        "\n",
        "\n",
        "digit_count = count_digits(num)\n",
        "print(f\"The number {num} has {digit_count} digits.\")\n"
      ],
