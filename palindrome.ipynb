      "source": [
        "\n",
        "def is_palindrome(value):\n",
        "\n",
        "    value_str = str(value)\n",
        "    return value_str == value_str[::-1]\n",
        "\n",
        "user_input = input(\"Enter a number or a string: \")\n",
        "\n",
        "if is_palindrome(user_input):\n",
        "    print(f\"'{user_input}' is a palindrome.\")\n",
        "else:\n",
        "    print(f\"'{user_input}' is not a palindrome.\")\n"
      ],
